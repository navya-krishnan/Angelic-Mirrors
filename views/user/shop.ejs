	<div class="product-section mt-150 mb-150">
		<div class="container">
			
			<div class="row">
				<div class="col-md-12">
					<div class="product-filters" >
						<ul>
							<li class="active" data-filter="*" me>All</li>
							<!-- <li data-filter=".strawberry">Wall Mirrors</li>
							<li data-filter=".berry">Floor Mirrors</li>
							<li data-filter=".lemon">Makeup Mirrors</li> -->
						</ul>
					</div>
				</div>
			</div>

			<div class="row product-lists">
				<%products.forEach((product,i)=>{%>
					<div class="col-lg-4 col-md-6 text-center <%= product.category %>">
						<div class="single-product-item">
							<div class="product-image">
								<a href="/singleProduct/<%= product._id %>">
									<img src="/image/<%= product.product_image[0]%>" alt="Product image">
								</a>
							</div>
							<h3>
								<%= product.product_name %>
							</h3>
							<h4>
								<%= product.product_category.category_name %>
							</h4>
							<p class="product-price">Rs.<%= product.product_price.toFixed(2) %>/-
							</p>

							<a href="/product/<%= product._id %>" class="cart-btn">View Product</a>
						</div>
					</div>
					<% }); %>
			</div>

